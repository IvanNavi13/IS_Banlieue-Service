<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.banlieueservice">
    
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/banlieueico"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/banlieueico_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BanlieueService"
        android:usesCleartextTraffic="true">

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCNQfVbH3VaFxKh3dnzZoQ_m8E4u_dVKHg" />



        <activity android:name=".actividades.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".usuario.PanelUsuarioActivity">
        </activity>

        <activity
            android:name=".repartidor.PanelRepartidorActivity">
        </activity>

        <activity
            android:name=".cliente.PanelClienteActivity">
        </activity>

        <activity
            android:name=".actividades.RegistroActivity">
        </activity>

        <activity
            android:name=".actividades.RegistroRepActivity">
        </activity>

        <activity
            android:name=".actividades.MapaActivity">
        </activity>
    </application>

</manifest>